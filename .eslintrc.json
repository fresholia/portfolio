{
  "extends": ["next/core-web-vitals", "eslint:recommended", "airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
      "prettier/prettier": "error",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "react/display-name": "off",
      "no-unused-vars": "off",
      "no-undef": "off"
  },
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  }
}
